$TTL    604800
@       IN      SOA     ns1.{{ domain }}. admin.{{ domain }}. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
     IN      NS      ns1.{{ domain }}.


; name servers - A records
ns1.{{ domain }}.          IN      A       {{ ip }}
ns2.{{ domain }}.          IN      A       {{ ip }}

; 10.128.0.0/16 - A records
host1.{{ domain }}.        IN      A      {{ ip }}
host2.{{ domain }}.        IN      A      {{ ip }}
