$TTL    604800
@       IN      SOA     ns1.{{ ns_domain }}. admin.{{ ns_domain }}. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
     IN      NS      ns1.{{ ns_domain }}.


; name servers - A records
ns1.{{ ns_domain }}.          IN      A       {{ ip }}
ns2.{{ ns_domain }}.          IN      A       {{ ip }}

; 10.128.0.0/16 - A records
host1.{{ ns_domain }}.        IN      A      {{ ip }}
host2.{{ ns_domain }}.        IN      A      {{ ip }}
